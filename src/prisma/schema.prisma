generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pedido {
  id        Int      @id @default(autoincrement())
  nome      String?
  itens     Json?
  total     Float
  codigoPix String
  status    String   @default("recebido")
  createdAt DateTime @default(now())
}
